{
  "manifest_version": 3,
  "name": "InstaLock",
  "version": "1.0",
  "description": "Only Messages",
  "action": {
    "default_icon": "icon.png",
    "default_title": "Instagram Reels Blocker",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["*://www.instagram.com/*"],
      "js": ["content.js"]
    }
  ]
}
